<?xml version="1.0" encoding="utf-8"?><androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:id="@+id/card"    android:layout_width="match_parent"    android:layout_height="100dp"    android:layout_marginTop="8dp"    app:cardCornerRadius="@dimen/dp15">    <androidx.constraintlayout.widget.ConstraintLayout        android:layout_width="match_parent"        android:layout_height="match_parent"        android:background="@color/gray_shimmer">        <TextView            android:id="@+id/tv_tittle"            style="@style/AppTheme.TextTittle"            android:layout_width="150dp"            android:layout_height="@dimen/dp20"            android:layout_marginStart="@dimen/dp15"            android:layout_marginTop="8dp"            android:layout_marginEnd="32dp"            android:background="@color/grey_font"            android:textStyle="bold"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toTopOf="parent" />        <TextView            android:id="@+id/tv_tittle2"            style="@style/AppTheme.TextTittle"            android:layout_width="120dp"            android:layout_height="@dimen/dp20"            android:layout_marginStart="@dimen/dp15"            android:layout_marginTop="4dp"            android:layout_marginEnd="32dp"            android:background="@color/grey_font"            android:textStyle="bold"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toBottomOf="@+id/tv_tittle" />        <TextView            android:id="@+id/tv_tittle3"            android:layout_width="100dp"            android:layout_height="@dimen/dp20"            android:layout_marginStart="@dimen/dp15"            android:layout_marginTop="4dp"            android:layout_marginEnd="32dp"            android:background="@color/grey_font"            android:textStyle="bold"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toBottomOf="@+id/tv_tittle2" />    </androidx.constraintlayout.widget.ConstraintLayout></androidx.cardview.widget.CardView>